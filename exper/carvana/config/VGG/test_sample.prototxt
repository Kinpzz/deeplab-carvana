  bottom: "fc8_res075_interp"
  bottom: "attention2_fc7"
  top: "fc8_res075_product" 
  name: "fc8_res075_product"
  type: "SpatialProduct"
}

layer {
  bottom: "fc8_res05_interp"
  bottom: "attention3_fc7"
  top: "fc8_res05_product" 
  name: "fc8_res05_product"
  type: "SpatialProduct"
}

### add features ###
layer {
  bottom: "fc8_product"
  bottom: "fc8_res075_product"
  bottom: "fc8_res05_product"
  top: "fc8_fusion"
  name: "fc8_fusion"
  type: "Eltwise"
  eltwise_param {
    operation: SUM
  }
}


################################
#   combine fc8 and mask7_up   #
################################

### upsample
layer {
  bottom: "fc8_fusion"
  top: "fc8_fusion_interp"
  name: "fc8_fusion_interp"
  type: "Interp"
  interp_param {
    zoom_factor: 8
    pad_beg: 0
    pad_end: 0
  }
}

### add features ###
layer {
  bottom: "fc8_fusion_interp"
  bottom: "output_fusion"

  top: "final_fusion"
  name: "final_fusion"
  type: "Eltwise"
  eltwise_param {
    operation: SUM
  }
}


##-----------
layer {
  name: "final_fusion_softmax"
  type: "Softmax"
  bottom: "final_fusion"
  top: "final_fusion"
}
##-----------
### compute loss  and accuracy

layer {
  name: "accuracy"
  type: "SegAccuracy"
  bottom: "final_fusion"
  bottom: "label"
  top: "accuracy-combine"
  seg_accuracy_param {
    ignore_label: 255
  } 
}

layer {
  name: "output_mask_mat"
  type: "MatWrite"
  bottom: "final_fusion"
  mat_write_param {
    prefix: "${FEATURE_DIR}/${TEST_SET}/fc8/"
    source: "${EXP}/list/${TEST_SET}_id.txt"
    strip: 0
    period: 1
  }
  include: { phase: TEST }
}

layer {
  name: "silence"
  type: "Silence"
  bottom: "data_dim"
  bottom: "label"
}
