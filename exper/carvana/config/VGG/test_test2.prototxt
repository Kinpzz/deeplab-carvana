# resolution_1
## fc7 -> mask1
layer {
  name: "mask1"
  type: "Convolution"
  bottom: "fc7"
  top: "mask1"
  
  param {
    name: "mask1_w"
    lr_mult: 1
    decay_mult: 1
  }
  param {
    name: "mask1_b"
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    num_output: 64
    kernel_size: 1
    stride: 1 
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }    
  }
}

layer {
  name: "mask1_relu"
  type: "ReLU"
  bottom: "mask1"
  top: "mask1"
}

